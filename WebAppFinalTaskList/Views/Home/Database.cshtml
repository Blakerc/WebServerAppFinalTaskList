@model IEnumerable<TaskModel>
@{
    ViewData["Title"] = "Database";
}
<!DOCTYPE html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Database</title>
</head>
<body>
<div>
    <h3>dbo.Tasks</h3>
    <table class="table table-bordered">  
    <tr>
        <th>Id</th>
        <th>UserId</th>
        <th>Title</th>  
        <th>Description</th>  
        <th>TaskDateTime</th>
    </tr>
    @foreach (var t in Model)  
    {  
        <tr>
            <td>@t.Id</td>
            <td>@t.UserId</td>
            <td>@t.Title</td>  
            <td>@t.Description</td>  
            <td>@t.TaskDateTime</td>
        </tr>  
    }
</table>
</div>
</body>